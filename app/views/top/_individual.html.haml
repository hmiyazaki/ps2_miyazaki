.col-sm-4.col-md-3.proto-content
  .thumbnail
    %a{:href => ""}
      = image_tag individual.prototype_images.find_by(status: "0").image.url
      .caption
        %h3
        = individual.title
        .proto-meta
          .proto-user
            = link_to individual.user.name, "users/#{individual.user.id}"
          .proto-posted
            = individual.updated_at
        %ul.proto-tag-list.list-inline
          %li 
            = link_to individual.tags.find_by(prototype_id: individual.id).tag_name, "#", class: "btn btn-default"
